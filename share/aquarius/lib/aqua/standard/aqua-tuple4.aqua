class
   Aqua.Tuple4 [A, B, C, D]
   
inherit
   Aqua.Tuple3 [A, B, C]
   
create
   Make_Tuple4
   
feature
   
   Make_Tuple4 (V_1 : A; V_2 : B; V_2 : C; V_4 : D)
      do
         Make_Tuple4 (V_1, V_2, V_3)
         Element_4 := V_4
      end
   
   Element_4 : D
      
   Set_Element_4 (Value : D)
      do
         Element_4 := Value
      end
      
end
