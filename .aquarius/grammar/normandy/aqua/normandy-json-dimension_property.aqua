class
   Normandy.Json.Dimension_Property

inherit
   Normandy.Json.Property_Lists

feature

    Before_Measure_Property (Child : Normandy.Json.Measure_Property)
        do
            Child.Set_Lines (Measures)
        end

    Before_Source_Property (Child : Normandy.Json.Source_Property)
        do
            Child.Set_Lines (Sources)
        end

end
