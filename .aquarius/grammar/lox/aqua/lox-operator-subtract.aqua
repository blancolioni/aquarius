class
    Lox.Operator.Subtract

inherit
    Lox.Operator

feature

    Apply (Left, Right : Lox.Value) : Lox.Value
        local
            Visitor : Lox.Value_Visitor.Subtract
        do
            create Visitor.Make (Right)
            Left.Accept (Visitor)
            Result := Visitor.Result_Value
        end
end
